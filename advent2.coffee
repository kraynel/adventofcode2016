_ = require 'lodash'

directions = [
  ''
  ''
  ''
  ''
  ''
]

# directions = [
#   'ULL'
#   'RRDDD'
#   'LURDL'
#   'UUUUD'
# ]

instructions =
  L: {x:-1, y:0}
  U: {x:0, y:-1}
  D: {x:0, y:1}
  R: {x:1, y:0}

posX = 0
posY = 2

digits = [
  ['', '', '1', '', '']
  ['', '2', '3', '4', '']
  ['5', '6', '7', '8', '9']
  ['', 'A', 'B', 'C', '']
  ['', '', 'D', '', '']
]

move = (instruction) ->
  m = instructions[instruction]
  if posX + m.x < 0 or posX + m.x > 4
    return
  if posY + m.y < 0 or posY + m.y > 4
    return
  if digits[posY+m.y][posX+m.x] is ''
    return
  posX += m.x
  posY += m.y
  return

_.map directions, (dir) ->
  moves = dir.split('')
  _.map moves, (m) ->
    move m
    # console.log digits[posY][posX]
  console.log 'AAA', digits[posY][posX]
  return
